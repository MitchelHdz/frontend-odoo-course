<?xml version="1.0" encoding="UTF-8"?>
<odoo>
	<template id="index">
		<t t-call="website.layout">
			<t t-set="title">Academy</t>
			<div class="oe_structure">
				<div class="container">
					<h1><t t-esc="user_id.name"/></h1>
					<t t-foreach="teachers" t-as="teacher">
						<p>
							<t t-esc="teacher.id"/>
							<t t-esc="teacher.name"/>
						</p>
					</t>	
				</div>
			</div>
		</t>
	</template>
</odoo>
